  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "eslint": "^8.57.1",
    "globals": "^16.2.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.0",
    "supertest": "^6.0.0",
    "prisma": "^5.0.0"
  }
